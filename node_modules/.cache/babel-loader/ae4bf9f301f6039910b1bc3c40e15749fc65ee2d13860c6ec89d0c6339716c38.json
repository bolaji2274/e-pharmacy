{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Header from \"@/components/common/Header.vue\";\nimport Footer from \"@/components/common/Footer.vue\";\nexport default {\n  components: {\n    Header,\n    Footer\n  },\n  data() {\n    return {\n      detail: this.$route.params\n    };\n  },\n  methods: {\n    goToCart() {\n      this.$router.push(\"/cart\");\n    }\n  },\n  created() {\n    if (this.$route.params.id !== undefined) localStorage.setItem(\"detail\", JSON.stringify(this.$route.params));\n  },\n  mounted() {\n    this.detail = JSON.parse(localStorage.getItem(\"detail\"));\n  }\n};","map":{"version":3,"mappings":";AA8BA;AACA;AAEA;EACAA;IACAC;IACAC;EACA;EACAC;IACA;MACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;EACA;EACAC;IACA,yCACAC;EACA;EACAC;IACA;EACA;AACA","names":["components","Header","Footer","data","detail","methods","goToCart","created","localStorage","mounted"],"sourceRoot":"src/components","sources":["drugDetail.vue"],"sourcesContent":["\n<template>\n  <div class=\" d-flex drug-detail-outer\">\n    <Header />\n    <div class=\"drug-detail\">\n        <div class=\"container mb-5\">\n            <h4 class=\"my-4 fw-600\">Add Item To Cart</h4>\n            <div class=\"row\">\n                <div class=\"col-md-4\" style=\"cursor: pointer;\">\n                <img :src=\"detail.url\" width=\"300px\" height=\"399px\" style=\"border-radius:15px;\">\n                </div>\n            <div class=\"col-md-4\" style=\"cursor: pointer;\">\n                <h5 class=\"my-4 fw-600\">Name: {{detail.name}}</h5>\n                 <h5 class=\"my-4 fw-600\">Price: {{detail.price}} </h5>\n                  <h5 class=\"my-4 fw-600\">Description: {{detail.description}}</h5>\n                   <h5 class=\"my-4 fw-600\">Rating: {{detail.rate}}</h5>\n                   <div>\n                    <button @click=\"goToCart\" class=\"btn btn-primary\">Go To Cart</button>\n                    <button class=\"btn btn-success\">Add Item</button>\n                    <button class=\"btn btn-danger\">Remove Item</button>\n                   </div>\n            </div>\n            </div>\n        </div>\n    </div>\n<Footer/>\n  </div>\n</template>\n\n<script>\nimport Header from \"@/components/common/Header.vue\"\nimport Footer from \"@/components/common/Footer.vue\"\n\nexport default {\n    components: {\n        Header,\n        Footer\n    },\n        data() {\n        return {\n            detail: this.$route.params\n        }\n    },\n    methods: {\n        goToCart(){\n            this.$router.push(\"/cart\");\n        }\n    },\n    created(){\n        if(this.$route.params.id !== undefined)\n            localStorage.setItem(\"detail\", JSON.stringify(this.$route.params));\n    },\n    mounted(){\n        this.detail = JSON.parse(localStorage.getItem(\"detail\"))\n    }\n}\n</script>\n\n<style scoped>\n.drug-detail-outer{\n    flex-direction: column;\n    height: 100vh;\n}\n.drug-details{\n    background: #efe9e2;\n    flex: 1;\n    overflow-y: auto;\n}\n.fw-600 {\n    font-weight: 600;\n}\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}