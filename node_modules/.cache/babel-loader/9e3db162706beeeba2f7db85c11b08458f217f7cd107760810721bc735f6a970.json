{"ast":null,"code":"import Navbar from '@/components/common/Navbar.vue';\nimport swal from \"sweetalert\";\nexport default {\n  name: \"Cart\",\n  components: {\n    Navbar\n  },\n  data() {\n    return {\n      item: 0\n    };\n  },\n  methods: {\n    addToCart(item) {\n      this.$store.dispatch(\"addToCart\", item);\n    },\n    removeItem(item) {\n      this.$store.dispatch(\"removeItem\", item);\n    },\n    checkout() {\n      swal(\"Good job!\", \"Your order is placed successfully!\", \"success\").then(value => {\n        window.location.href = \"/cart\";\n      });\n    }\n  },\n  computed: {\n    cartItems() {\n      return this.$store.state.cartItem;\n    },\n    totalPrice() {\n      let price = 0;\n      this.$store.state.cartItem.map(el => {\n        price += el[\"quantity\"] * el[\"price\"];\n      });\n      return price;\n    }\n  }\n};","map":{"version":3,"mappings":"AAkEA;AACA;AAEA;EACAA;EACAC;IACAC;EACA;EACAC;IACA;MACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACAC,wEACAC;QACAC;MACA,EACA;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;MACA;QACAC;MACA;MACA;IACA;EACA;AACA","names":["name","components","Navbar","data","item","methods","addToCart","removeItem","checkout","swal","value","window","computed","cartItems","totalPrice","price"],"sourceRoot":"src/components","sources":["Cart.vue"],"sourcesContent":["<template>\n  <div class=\"d-flex cart-outer-div\">\n    <!-- <Header /> -->\n    <div class=\"cart-body\">\n      <div v-if=\"totalPrice !== 0\" class=\"container mb-5\">\n        <h4 class=\"my-4 my-cart\">My Cart</h4>\n        <div class=\"d-flex my-3\" style=\"justify-content: space-between;\">\n          <h4 class=\"fw-600\">Summary</h4>\n          <h4 class=\"fw-600\" style=\"margin-right: 49%;\">Cart</h4>\n        </div>\n        <div class=\"d-flex\">\n          <div v-show=\"totalPrice !== 0\" class=\"row\" style=\"width: 50%\">\n            <div class=\"col-md-4\">\n              <h6>Total:</h6>\n              <h6>Promo Code:</h6>\n              <h6>Shipping:</h6>\n              <div class=\"my-3 line w-120\"></div>\n              <h6>Subtotal:</h6>\n            </div>\n            <div class=\"col-md-4\">\n              <h6>${{totalPrice}}.00</h6>\n              <h6>HB-drugs</h6>\n              <h6>$50.00</h6>\n              <div class=\"my-3 line w-60\"></div>\n              <h6>${{totalPrice + 50}}.00</h6>\n            </div>\n          </div>\n          <div class=\"row\" style=\"width: 50%\">\n            <div style=\"max-width: 70%\" class=\"col-md-12\">\n              <ul style=\"padding: 0\">\n                <li v-for=\"items in cartItems\" :key=\"items.id\" style=\"list-style: none;\">\n                  <div class=\"cart-items\">\n                    <img :src=\"items.url\" width=\"50px\" height=\"50px\" style=\"border-radius: 50%\" />\n                    <h6 class=\"mt-15\">{{items.name}}</h6>\n                    <div class=\"d-flex mt-10\">\n                      <button v-on:click=\"removeItem(items)\" class=\"add\" type=\"button\">-</button>\n                      <span class=\"cart-quantity\">{{items.quantity}}</span>\n                      <button v-on:click=\"addToCart(items)\" class=\"remove\" type=\"button\">+</button>\n                    </div>\n                    <h6 class=\"mt-15\">${{items.price * items.quantity}}.00</h6>\n                  </div>\n                  <div class=\"line\"></div>\n                </li>\n              </ul>\n            </div>\n          </div>\n        </div>\n        <div class=\"d-flex justify-content-end\" style=\"width: 80%; margin-top: 2%\">\n          <button v-on:click=\"checkout\" class=\"btn btn-primary\" type=\"button\">Checkout</button>\n        </div>\n      </div>\n      <div v-else class=\"container empty\" style=\"text-align: center\">\n        <div class=\"nav-bar\">\n          <Navbar/>\n        </div>\n        <div class=\"text-content\">\n\n        </div>\n        \n      </div>\n    </div>\n    <!-- <Footer /> -->\n  </div>\n</template>\n\n<script>\nimport Navbar from '@/components/common/Navbar.vue'\nimport swal from \"sweetalert\";\n\nexport default {\n  name: \"Cart\",\n  components: {\n    Navbar\n  },\n  data() {\n    return {\n      item: 0\n    };\n  },\n  methods: {\n    addToCart(item) {\n      this.$store.dispatch(\"addToCart\", item);\n    },\n    removeItem(item) {\n      this.$store.dispatch(\"removeItem\", item);\n    },\n    checkout() {\n      swal(\"Good job!\", \"Your order is placed successfully!\", \"success\").then(\n        value => {\n          window.location.href = \"/cart\";\n        }\n      );\n    }\n  },\n  computed: {\n    cartItems() {\n      return this.$store.state.cartItem;\n    },\n    totalPrice() {\n      let price = 0;\n      this.$store.state.cartItem.map(el => {\n        price += el[\"quantity\"] * el[\"price\"];\n      });\n      return price;\n    }\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.cart-outer-div {\n  flex-direction: column;\n  height: 100vh;\n}\n.cart-body {\n  background: #efe9e2;\n  flex: 1;\n  overflow-y: auto;\n}\n.cart-quantity {\n  display: inline-block;\n  padding: 3px 6px;\n  width: 46px;\n  height: 28px;\n  border-radius: 2px;\n  background-color: #fff;\n  margin: 0 5px;\n  text-align: center;\n}\n.cart-items {\n  display: flex;\n  justify-content: space-between;\n  padding: 10px;\n  background: white;\n}\n.my-cart {\n  color: orange;\n  font-weight: 600;\n}\ninput[type=\"text\"] {\n  border: none;\n  width: 100%;\n  font-size: 14px;\n  font-weight: 500;\n  vertical-align: middle;\n  text-align: center;\n  outline: none;\n}\n.fw-600 {\n  font-weight: 600;\n}\n.line {\n  height: 1px;\n  border-bottom: 1px solid black;\n}\n.w-120 {\n  width: 120%;\n}\n.w-60 {\n  width: 60%;\n}\n.add,\n.remove {\n  width: 28px;\n  height: 28px;\n  background: linear-gradient(#fff, #f9f9f9);\n  border: 1px solid #c2c2c2;\n  cursor: pointer;\n  font-size: 16px;\n  border-radius: 7px;\n  padding-top: 1px;\n  outline: none;\n}\n.mt-10 {\n  margin-top: 10px;\n}\n.mt-15 {\n  margin-top: 15px;\n}\n.orange-red {\n  color: orangered;\n}\n.darkblue {\n  color: darkblue;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}