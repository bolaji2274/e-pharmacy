{"ast":null,"code":"export default {\n  data() {\n    return {\n      userData: {\n        email: '',\n        password: '',\n        age: 22\n      },\n      message: 'A new Text arises',\n      sendMail: [],\n      gender: 'male',\n      priorities: ['High', 'Medium', 'Low']\n    };\n  }\n};","map":{"version":3,"mappings":"AAmIA;EACAA;IACA;MACAC;QACAC;QACAC;QACAC;MACA;MACAC;MACAC;MACAC;MACAC;IACA;EACA;AACA","names":["data","userData","email","password","age","message","sendMail","gender","priorities"],"sourceRoot":"src/components/Form","sources":["UserInput.vue"],"sourcesContent":["<template>\n    <div class=\"container\">\n        <form>\n            <div class=\"row\">\n                <div class=\"col-xs-12 col-sm-8 col-sm-offset-2 col-md-6 col-md-offset-3\">\n                    <h1>File a Complaint</h1>\n                    <hr>\n                    <div class=\"form-group\">\n                        <label for=\"email\">Mail</label>\n                        <input\n                                type=\"text\"\n                                id=\"email\"\n                                class=\"form-control\"\n                                v-model=\"userData.email\">\n                    </div>\n                    <div class=\"form-group\">\n                        <label for=\"password\">Password</label>\n                        <input\n                                type=\"password\"\n                                id=\"password\"\n                                class=\"form-control\"\n                                v-model.lazy.trim=\"userData.password\">\n                                <h3>{{userData.password}}</h3>\n                    </div>\n                    <div class=\"form-group\">\n                        <label for=\"age\">Age</label>\n                        <input\n                                type=\"number\"\n                                id=\"age\"\n                                class=\"form-control\"\n                     